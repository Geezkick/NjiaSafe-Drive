const options = {
    method: 'GET',
    headers: {accept: 'application/json', 'accept-encoding': 'deflate, gzip, br'}
  };
  
  fetch('https://api.tomorrow.io/v4/weather/forecast?location=new%2520york&apikey=we', options)
    .then(res => res.json())
    .then(res => console.log(res))
    .catch(err => console.error(err));